        binding="basicHttpBinding" bindingConfiguration="DeltekVisionOpenAPIWebServiceSoap"
        contract="VisionAPI.DeltekVisionOpenAPIWebServiceSoap" name="DeltekVisionOpenAPIWebServiceSoap" />
    </client>
